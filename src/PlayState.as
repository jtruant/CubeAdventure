		//function to create platforms
		public function createMovingPlatform(X:uint,Y:uint,width:uint,height:uint,accel:FlxPoint,maxVelocity:FlxPoint):void
		{
			/*instantiate object*/
			var platform:FlxSprite = new FlxSprite(X*8+3,Y*8+2);
			platform.makeGraphic(width,height,FlxG.BLACK);
			/*set attributes*/
			platform.acceleration =accel;
			platform.maxVelocity = maxVelocity;
			/*add to group*/
			platforms.add(platform);
		}
		
		public function ReversePlatformMovement(level:FlxTilemap,platform:FlxSprite):void
		{   
			if(platform.acceleration.x > 0)
			{
				platform.acceleration.x = -platform.maxVelocity.x;
			}
			else if(platform.acceleration.x<0)
			{
				platform.acceleration.x = platform.maxVelocity.x;
			}
			if(platform.acceleration.y > 0)
			{
				platform.acceleration.y= -platform.maxVelocity.y;
			}
			else if(platform.acceleration.y<0)
			{
				platform.acceleration.y = platform.maxVelocity.y;
			}
		}
		
		/*checks if specified FlxSprite has left the game bounds*/
		public function CheckBounds(sprite:FlxSprite):Boolean
		{
			if(sprite.x > FlxG.width || sprite.x<0 || 
				sprite.y > FlxG.height || sprite.y<0)
			{
				return true;
			}
			else
			{
			   return false;
			}
		}
		

		
		public function MaintainBoundaries(sprite:FlxSprite):void
		{
			if(CheckBounds(sprite))
			{
				if(sprite.x > FlxG.width)
				{
					sprite.acceleration.x = -sprite.maxVelocity.x;
				}
				if(sprite.x<0)
				{
					sprite.acceleration.x = sprite.maxVelocity.x;
				}
				if(sprite.y>FlxG.height)
				{
					sprite.acceleration.y = -sprite.maxVelocity.y;
				}
				if(sprite.y<0)
				{
				    sprite.acceleration.y = sprite.maxVelocity.y;
				}
			}
		}

		public function MaintainBoundariesOfGroup(sprites:FlxGroup):void
		{
			var i:int = 0;
			
			for (i<sprites.members.length; i++;)
			{
				var sprite:FlxSprite = new FlxSprite();
				sprite = sprites.members[i];
				
				if(CheckBounds(sprite))
				{
					if(sprite.x > FlxG.width)
					{
						sprite.acceleration.x = -sprite.maxVelocity.x;
					}
					if(sprite.x<0)
					{
						sprite.acceleration.x = sprite.maxVelocity.x;
					}
					if(sprite.y>FlxG.height)
					{
						sprite.acceleration.y = sprite.maxVelocity.y;
					}
					if(sprite.y<0)
					{
						sprite.acceleration.y = -sprite.maxVelocity.y;
					}
				}
			}
			
			
		}
		
