		//function to update on screen objects
		override public function update():void
		{
			//starting accerleration
			screenChanging = false;
			player.acceleration.x=0;
			
			if(FlxG.keys.LEFT)
			{
				player.acceleration.x=-player.maxVelocity.x*4;
			}
			if(FlxG.keys.RIGHT)
			{
				player.acceleration.x=player.maxVelocity.x*4;
			}
			if(FlxG.keys.DOWN)
			{
				player.makeGraphic(10,6,0xffaa1111);
				ducking = true;
			}
			if(FlxG.keys.UP)
			{
				if(ducking ==true)
				{
					player.y=player.y-2;
					player.makeGraphic(10,12,0xffaa1111);
					ducking = false;
				}
			}
			/*double jump if player has power up*/
			if(FlxG.keys.SPACE && player.isTouching(FlxObject.FLOOR) && doubleJumpPower)
			{	
				player.velocity.y=-player.maxVelocity.y/2;
				
				//This second 'if' statement checks that we've stopped doing our first jump, and sets us up to do a double jump.
				if ((FlxG.keys.justReleased("SPACE") && SINGLEJUMP) || player.velocity.y == 0)
				{
					DOUBLEJUMP = true;
					SINGLEJUMP = false;
				}
				
				//This final 'if' statement is our mid-air, or double jump
				if (FlxG.keys.justPressed("SPACE") && (player.velocity.y > 0 || player.velocity.y < 0) && DOUBLEJUMP)
				{
					//velocity.y = -JUMPSPEED / 1.4;
					player.velocity.y = -player.maxVelocity.y / 4;
					DOUBLEJUMP = false;
					
					
				}
				
			}
			if(FlxG.keys.SPACE && !doubleJumpPower && player.isTouching(FlxObject.FLOOR))
			{
				player.velocity.y=-player.maxVelocity.y/2;
			}
			
			if(FlxG.keys.R)
			{
				currentScreenCount = 1;
			    FlxG.resetGame();
			}
			if(FlxG.keys.C)
			{
				//currentScreenCount++;
				UpdateScreen(forwardExit,player);
			}
			//Check for player lose conditions
			if(player.y > FlxG.height)
			{
				/*change to loss state*/
				FlxG.switchState(new LossState(score));
			}
			
			/*update screen based on player position*/
			super.update();
			
			FlxG.overlap(player,coins,GetCoin);
			FlxG.overlap(player,treasure2,GetTreasure);
			FlxG.collide(player,currentScreen);
			FlxG.collide(player,platforms);
			FlxG.collide(currentScreen,platforms,ReversePlatformMovement);
			MaintainBoundariesOfGroup(platforms);
			
			//platforms.callAll("MaintainBoundaries");
			//change screen if player hits an exit or door
			FlxG.overlap(exits,player,UpdateScreen);
		}
		
